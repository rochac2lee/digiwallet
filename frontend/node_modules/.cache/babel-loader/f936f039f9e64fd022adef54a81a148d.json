api.patch(\n        `${endpoint}/${id}`,\n        payload,\n        config()\n    )\n    .then(response)\n    .catch(reject)\n\nconst httpDelete = (endpoint, id, response, reject) =>\n    api.delete(\n        `${endpoint}/${id}`,\n        config()\n    )\n    .then(response)\n    .catch(reject)\n\nexport default {\n    baseUrl: `http://${API_HOST}/${API_REST}`,\n    post: httpPost,\n    get: httpGet,\n    put: httpPut,\n    patch: httpPatch,\n    delete: httpDelete\n}"]}]}