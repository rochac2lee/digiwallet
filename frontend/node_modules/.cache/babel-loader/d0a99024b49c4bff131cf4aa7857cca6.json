\n    }\n\n}\n\nconst actions = {\n    efetuarLogin({ commit }, usuario) {\n        return new Promise((resolve, reject) => {\n            http.post(\n                \"auth/login\",\n                usuario,\n                res => {\n                    if (res.status === 200 && res.data.token) {\n                        this.usuario = res.data.usuario\n                        console.log(res.data);\n                        Vue.$cookies.set('usuario_id', this.usuario.id)\n                        Vue.$cookies.set('cliente_id', this.usuario.cliente_id)\n                        Vue.$cookies.set('nome', this.usuario.nome)\n                        Vue.$cookies.set('perfil', md5(this.usuario.perfil))\n                        resolve(res.data)\n\n                        commit('DEFINIR_USUARIO_LOGADO', {\n                            token: res.data.token,\n                            usuario: res.data.usuario\n                        })\n                    }\n                },\n                err => {\n                    console.error(err)\n                    reject(err)\n                }\n            )\n        })\n    }\n}\n\nconst getters = {\n    usuarioEstaLogado: state => Boolean(state.token)\n}\n\nexport default new Vuex.Store({\n\n    state,\n    mutations,\n    actions,\n    getters\n\n})"]}]}